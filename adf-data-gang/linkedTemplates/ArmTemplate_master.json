{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-data-gang"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"ls_susbecant_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_susbecant'"
		},
		"ls_datagang_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadatagang.dfs.core.windows.net"
		},
		"ls_key_data_gang_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-datagang.vault.azure.net/"
		},
		"ls_susbecant_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://susbecantt.dfs.core.windows.net/"
		},
		"triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_container": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_folder": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"triggerEvent_SIASUS_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		},
		"triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_container": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_folder": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"triggereEvent_SIHSUS_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d44271f6-fec8-44b4-b79f-a0fd742c7602/resourceGroups/susbecantt/providers/Microsoft.Storage/storageAccounts/susbecantt"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-data-gang_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"ls_susbecant_accountKey": {
						"value": "[parameters('ls_susbecant_accountKey')]"
					},
					"ls_key_data_gang_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_key_data_gang_properties_typeProperties_baseUrl')]"
					},
					"ls_susbecant_properties_typeProperties_url": {
						"value": "[parameters('ls_susbecant_properties_typeProperties_url')]"
					},
					"ls_datagang_properties_typeProperties_url": {
						"value": "[parameters('ls_datagang_properties_typeProperties_url')]"
					},
					"triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_container": {
						"value": "[parameters('triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_container')]"
					},
					"triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_folder": {
						"value": "[parameters('triggerEvent_SIASUS_properties_pipeline_copy_SIASUS_parameters_folder')]"
					},
					"triggerEvent_SIASUS_properties_typeProperties_scope": {
						"value": "[parameters('triggerEvent_SIASUS_properties_typeProperties_scope')]"
					},
					"triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_container": {
						"value": "[parameters('triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_container')]"
					},
					"triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_folder": {
						"value": "[parameters('triggereEvent_SIHSUS_properties_pipeline_copy_SIHSUS_parameters_folder')]"
					},
					"triggereEvent_SIHSUS_properties_typeProperties_scope": {
						"value": "[parameters('triggereEvent_SIHSUS_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}